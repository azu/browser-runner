    "optionator": "^0.6.0",
    "selenium-webdriver": "^2.46.1"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "espower-babel": "^3.2.0",
    "mocha": "^2.2.5",
    "power-assert": "^0.11.0"
  }
}
