  "version": "1.1.0",
  "description": "Run browsers via WebDriver API.",
  "main": "lib/browser-runner.js",
  "bin": {
    "browser-runner": "bin/cmd.js"
  },
  "files": [
    "bin",
    "lib"
  ],
